{
  "name": "@12luckydev/utils",
  "version": "2.2.1",
  "description": "Utils for working with arrays!",
  "keywords": [
    "array",
    "add",
    "edit",
    "delete"
  ],
  "source": "src/index.ts",
  "main": "lib/cjs/index.js",
  "types": "lib/cjs/index.d.ts",
  "files": [
    "lib"
  ],
  "exports": {
    ".": {
      "types": "./lib/types/index.d.ts",
      "require": "./lib/cjs/index.js",
      "import": "./lib/esm/index.js",
      "default": "./lib/esm/index.js"
    }
  },
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "npm-run-all build:clean build:cjs build:esm build:types",
    "build:clean": "rimraf lib",
    "build:cjs": "tsc -p tsconfig.cjs.json && echo {\"type\": \"commonjs\"} > lib/cjs/package.json",
    "build:esm": "tsc -p tsconfig.esm.json && echo {\"type\": \"module\"} > lib/esm/package.json",
    "build:types": "tsc -p tsconfig.types.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint .",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "author": "12luckydev",
  "license": "MIT",
  "repository": "12LuckyDev/utils",
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@types/eslint__js": "^8.42.3",
    "@types/jest": "^29.5.12",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.4",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.12.2"
  }
}
